{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "delegateroute": {
        "ClusterId": "delegatecluster",
        "Match": {
          "Path": "/delegate/{**catch-all}"
        }
      },
      "proxyroute": {
        "ClusterId": "proxycluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "delegatecluster": {
        "Destinations": {
          "SampleHttpSysServer": {
            "Address": "http://localhost:5600/",
            "Metadata": {
              "HttpSysQueueName": "SampleHttpSysServerQueue"
            }
          }
        }
      },
      "proxycluster": {
        "Destinations": {
          "httpbin.org": {
            "Address": "https://httpbin.org/"
          }
        }
      }
    }
  }
}
